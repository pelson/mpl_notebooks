     "input": [
      "\"\"\"\n",
      "Demo of the errorbar function.\n",
      "\"\"\"\n",
      "import numpy as np\n",
      "import matplotlib.pyplot as plt\n",
      "\n",
      "# example data\n",
      "x = np.arange(0.1, 4, 0.5)\n",
      "y = np.exp(-x)\n",
      "\n",
      "plt.errorbar(x, y, xerr=0.2, yerr=0.4)\n",
      "plt.show()"
